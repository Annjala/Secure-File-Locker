{
  "expo": {
    "name": "SecureFileLocker",
    "slug": "secure-file-locker",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "package": "com.securefilelocker.app"
    },
    "plugins": [
      "expo-asset",
      "expo-font"
    ]
  }
}
